!ELSEIF $(IA64)

TARGETLIBS=  $(SDXROOT)\public\sdk\lib\ia64\user32.lib $(SDXROOT)\public\sdk\lib\ia64\setupapi.lib $(SDXROOT)\public\sdk\lib\ia64\version.lib

