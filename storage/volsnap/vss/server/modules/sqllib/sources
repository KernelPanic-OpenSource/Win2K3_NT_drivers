TARGETNAME=sqllib
TARGETPATH=..\..\lib\$(_OBJ_DIR)
TARGETTYPE=LIBRARY

USE_MSVCRT = 1
USE_NATIVE_EH = ASYNC
USE_ATL=1
ATL_VER=30
USE_STL=1

!if $(FREEBUILD)
C_DEFINES=-D_MT -DWIN_32 -DWIN32 -D_WINDOWS -DNT -D_WIN32_DCOM -DUNICODE -D_UNICODE -D_ATL_NO_DEBUG_CRT
!else
C_DEFINES=-D_MT -DWIN_32 -DWIN32 -D_WINDOWS -DNT -D_WIN32_DCOM -DUNICODE -D_UNICODE -D_ATL_NO_DEBUG_CRT -D_DEBUG
!endif



INCLUDES=inc;..\..\inc;..\..\inc\derived

SOURCES= sqlconnect.cpp \
	 snapsql.cpp \
	 sqlenum.cpp \
	 vdifreeze.cpp
	

PRECOMPILED_CXX = 1
PRECOMPILED_INCLUDE = stdafx.h
