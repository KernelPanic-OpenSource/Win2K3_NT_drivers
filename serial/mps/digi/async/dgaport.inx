;****************************************************************************************************
; Install section
;****************************************************************************************************
[DigiPort.NT]
AddReg=DigiPort.AddReg
; addreg and copy files for serenum.sys
Include=msports.inf
Needs=SerialEnumerator.NT

[DigiPort.NT.HW]
AddReg=DigiPortHW.AddReg

[DigiPort.NT.Services]
AddService=DIGIFEP5,%SPSVCINST_ASSOCSERVICE%,DIGIFEP5.AddService,DIGIFEP5.AddEventLog
; install the serenum service itself
Include=msports.inf
Needs=SerialEnumerator.NT.Services

[DigiPort.NT.CoInstallers]
CopyFiles=DigiPort.CoInstallers.CopyFiles
AddReg=DigiPort.CoInstallers.AddReg

;***************************************************************************************************
; CopyFiles Sections
;****************************************************************************************************
[DigiPort.CoInstallers.CopyFiles]

;[SourceDisksNames]

;[SourceDisksFiles]

;***************************************************************************************************
; AddReg Sections
;****************************************************************************************************
[DigiPort.CoInstallers.AddReg]
HKR,,CoInstallers32,%REG_MULTI_SZ%,"dgconfig.dll,DigiPortCoInstaller"

[DigiPort.AddReg]
HKR,,PortDriver,,digifep5.sys

[DigiPortHW.AddReg]
HKR,,"UpperFilters",0x00010000,"serenum"

;****************************************************************************************************
; Services
;****************************************************************************************************
[DIGIFEP5.AddService]
DisplayName=%Digifep5.ServiceDesc%
ServiceType=%SERVICE_KERNEL_DRIVER%
StartType=%SERVICE_DEMAND_START%
ErrorControl=%SERVICE_ERROR_NORMAL%
ServiceBinary=%12%\digifep5.sys     ; 12=drivers directory
LoadOrderGroup=Extended base

[DIGIFEP5.AddEventLog]
AddReg=DIGIFEP5.AddEventLog.AddReg

[DIGIFEP5.AddEventLog.AddReg]
HKR,,EventMessageFile,%REG_EXPAND_SZ%,"%%SystemRoot%%\System32\IoLogMsg.dll;%%SystemRoot%%\System32\drivers\digifep5.sys"
HKR,,TypesSupported,%REG_DWORD%,7
