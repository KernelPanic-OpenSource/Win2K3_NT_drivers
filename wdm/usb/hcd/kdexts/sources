NTPUBLIC=$(_NTDRIVE)$(_NTROOT)\public

INCLUDES= ..\inc;\
          $(BASE_INC_PATH);\
          $(DDK_INC_PATH);\
          $(DDK_INC_PATH)\wdm;

USE_MSVCRT=1

UMTYPE=windows

# ICF doesn't do any good for this image.  Turn it off.
LINKER_NOICF=1

